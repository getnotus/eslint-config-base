{
  "name": "@notus/eslint-config-base",
  "version": "0.1.6",
  "description": "Notus's base TS ESLint config, following our styleguide",
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "styleguide"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/getnotus/eslint-config-base"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "check-prettier": "npx eslint-config-prettier index.js",
    "check-prettier-test": "npx eslint-config-prettier test/index.test.js",
    "eslint-find-option-rules": "eslint-find-rules [option] <file> [flag]",
    "lint": "eslint --report-unused-disable-directives .",
    "plugin": "eslint-find-rules --plugin",
    "unused": "eslint-find-rules --unused"
  },
  "dependencies": {
    "confusing-browser-globals": "^1.0.9",
    "eslint-config-prettier": "^8.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.2.0",
    "eslint-find-rules": "^4.0.0",
    "eslint-plugin-array-func": "^3.1.6",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.2.0",
    "eslint-plugin-array-func": "^3.1.6",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  }
}
